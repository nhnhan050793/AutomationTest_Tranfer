<!-- Chạy tuần tự hàng đợi -->
<!-- <suite name="Bank Guru" parallel="false"> -->

<!-- Chạy song song 2 thằng cùng lúc trên 1 máy, thread-count="3" thì 3 cái 
	chạy cùng lúc -->
<suite name="Bank Guru" parallel="false" thread-count="2">

	<listeners>
		<!-- MethodListener -->
		<listener class-name="commons.MethodListener" />
		
		<!-- Testng Report -->
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		
		<!-- Jenkins Report -->
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		
		<!-- ExtentReport V2 -->
		<!-- <listener class-name="reportConfig.ExtentReportListener" /> -->
		
		<!-- ExtentReport V3 -->
		<!-- <listener class-name="reportConfig.ExtentTestListener" /> -->
		
		<!-- ExtentReport V4 -->
		<listener class-name="reportConfig.ExtentListener" />
		
		<!-- Allure Report -->
		<listener class-name="reportConfig.AllureTestListener" />
		
		<!-- retry testcase fail -->
		<listener class-name="retryConfig.RetryListener" />
		
	</listeners>
	
